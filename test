    -DCMAKE_CXX_COMPILER=/opt/rocm/llvm/bin/clang++ \
    -DCMAKE_C_COMPILER=/opt/rocm/llvm/bin/clang \
    -DLLVM_DIR=/opt/rocm/llvm/lib/cmake/llvm \
    -DBUILD_SHARED_LIBS=ON \
    -DCMAKE_BUILD_TYPE=Release \
    -DCOMGR_LLVM_LINK_STATIC=ON

ninja
